  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && cross-env BULD_MODE=prod node ./build/src/server.js",
    "dev": "cross-env BULD_MODE=dev nodemon --exec babel-node ./src/server.js"
  },
  "devDependencies": {
