    "nodemon": "^3.1.10",
    "prettierrc": "^0.0.0-5"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "async-exit-hook": "^2.0.1",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
